 window.AnchorPoint = Vector2.new(0.5,0.5)
 window.Position = UDim2.new(0.5,0,0.5,0)
 window.BackgroundColor3 = THEME.window
 window.Parent = gui
roundify(window,18)
stroke(window, THEME.stroke, 1, .92)

