-- POPCAT x HUB - v4.0 (Dynamic Nav Width, No-Blur, Text-Safe, Symmetric)
-- Clean rebuild to fix syntax errors and add requested behavior.
--  * No Lighting/Blur usage
--  * F8 open/close uses slide (no scale) -> crisp text
--  * Left nav auto-expands to fit long titles; right content reflows
--  * Symmetric top spacing; content buttons account for right grip
--  * No ripple on grip; ripple kept on buttons

local Players      = game:GetService("Players")
local UIS          = game:GetService("UserInputService")
local TweenService = game:GetService("TweenService")
local TextService  = game:GetService("TextService")
local LP           = Players.LocalPlayer

---------------------------------------------------------------------
-- THEME
---------------------------------------------------------------------
local THEME = {
  window      = Color3.fromRGB(0,0,0),        -- solid black back layer
  panel       = Color3.fromRGB(36,43,47),     -- lighter panels (nav & content)
  card        = Color3.fromRGB(26,32,35),     -- button bg
  cardHover   = Color3.fromRGB(33,41,45),
  cardActive  = Color3.fromRGB(41,49,53),
  text        = Color3.fromRGB(235,240,245),
  subtext     = Color3.fromRGB(170,180,185),
  stroke      = Color3.fromRGB(255,255,255),
  selBar      = Color3.fromRGB(70,160,255),
}

---------------------------------------------------------------------
-- HELPERS
---------------------------------------------------------------------
local function roundify(gui, r)
  local c = Instance.new("UICorner")
  c.CornerRadius = UDim.new(0, r or 16)
  c.Parent = gui
  return c
end

local function stroke(gui, col, th, tr)
  local s = Instance.new("UIStroke")
  s.Color = col or THEME.stroke
  s.Thickness = th or 1
  s.Transparency = tr or .85
  s.ApplyStrokeMode = Enum.ApplyStrokeMode.Border
  s.Parent = gui
  return s
end

local function t(obj, info, props)
  TweenService:Create(obj, info, props):Play()
end

local function spring(dur)
  return TweenInfo.new(dur or 0.22, Enum.EasingStyle.Quint, Enum.EasingDirection.Out)
end

local function ripple(parent, pos)
  local r = Instance.new("Frame")
  r.BackgroundColor3 = Color3.fromRGB(255,255,255)
  r.BackgroundTransparency = 0.8
  r.AnchorPoint = Vector2.new(0.5,0.5)
  r.Position = UDim2.fromOffset(pos.X, pos.Y)
  r.Size = UDim2.fromOffset(0,0)
  r.ZIndex = (parent.ZIndex or 1) + 1
  r.Parent = parent
  roundify(r, 9999)
  local max = math.max(parent.AbsoluteSize.X, parent.AbsoluteSize.Y) * 1.6
  t(r, TweenInfo.new(0.35, Enum.EasingStyle.Sine, Enum.EasingDirection.Out), {Size = UDim2.fromOffset(max,max), BackgroundTransparency = 1})
  game.Debris:AddItem(r, 0.45)
end

local function hover(btn, normal, over)
  btn.MouseEnter:Connect(function() t(btn, spring(0.16), {BackgroundColor3 = over}) end)
  btn.MouseLeave:Connect(function() t(btn, spring(0.18), {BackgroundColor3 = normal}) end)
end

local function fadeIn(obj, dur)
  dur = dur or 0.18
  if obj:IsA("TextLabel") or obj:IsA("TextButton") then
    obj.TextTransparency = 1
    t(obj, spring(dur), {TextTransparency = 0})
  end
  if obj:IsA("ImageLabel") or obj:IsA("ImageButton") then
    obj.ImageTransparency = 1
    t(obj, spring(dur), {ImageTransparency = 0})
  end
  if obj:IsA("GuiObject") and obj.BackgroundTransparency < 1 then
    local b = obj.BackgroundTransparency
    obj.BackgroundTransparency = 1
    t(obj, spring(dur), {BackgroundTransparency = b})
  end
end

