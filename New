-- Controls
local controls = Instance.new("Frame")
 controls.AnchorPoint = Vector2.new(1,0)
 controls.Position = UDim2.new(1,-10,0,6)
 controls.Size = UDim2.fromOffset(150,32)
 controls.BackgroundTransparency = 1
 controls.Parent = titleBar

local function ctrlBtn(sym)
  local b = Instance.new("TextButton")
  b.AutoButtonColor=false
  b.Size = UDim2.fromOffset(40,28)
  b.BackgroundColor3 = THEME.card
  b.Text = sym
  b.TextSize = 18
  b.Font = Enum.Font.GothamBold
  b.TextColor3 = Color3.fromRGB(215,220,225)
  roundify(b,10)
  local s = stroke(b, THEME.stroke, 1, .82)
  hover(b, THEME.card, THEME.cardHover)
  b.MouseButton1Down:Connect(function(input)
    ripple(b, Vector2.new(input.Position.X - b.AbsolutePosition.X, input.Position.Y - b.AbsolutePosition.Y))
    t(s, TweenInfo.new(0.12), {Transparency = 0.5})
  end)
  b.MouseButton1Up:Connect(function()
    t(s, TweenInfo.new(0.16), {Transparency = 0.82})
  end)
  return b
end

local btnMin = ctrlBtn("-"); btnMin.Position = UDim2.fromOffset(0,2);   btnMin.Parent = controls
local btnMax = ctrlBtn("□"); btnMax.Position = UDim2.fromOffset(50,2);  btnMax.Parent = controls
local btnClose = ctrlBtn("X"); btnClose.Position = UDim2.fromOffset(100,2); btnClose.TextColor3 = Color3.fromRGB(230,80,80); btnClose.Parent = controls
-- simgeleri büyüt (buton boyutları aynı kalır)
btnMin.TextSize = 30
btnMax.TextSize = 30
btnClose.TextSize = 28
