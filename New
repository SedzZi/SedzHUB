local currentNavW = NAV_MIN

-- Left nav
local nav = Instance.new("Frame")
 nav.Size = UDim2.new(0,currentNavW,1,0)
 nav.BackgroundColor3 = THEME.panel
 nav.Parent = body
roundify(nav,14)
